{
    "global_options": {
        "name": "test-64-nodes",
        "numnodes": "64",
        "ppn": "1",
        "allocationmode": "p",
        "partitionsplit": "e",
        "allocationsplit": "e",
        "minruns": "1",
        "maxruns": "1",
        "timeout": "1800.0",
        "convergeall": true,
        "outformat": "csv",
        "extrainfo": "64nodes_limit_test",
        "sbatch_directives": {
            "account": "IscrB_SWING",
            "partition": "boost_usr_prod",
            "time": "00:30:00",
            "job-name": "crab_64n_test"
        }
    },
