        "08_scale_a2a_1GB": {
            "description": "Scalability Test 8/8: 1GB msg size (Stress Bandwidth).",
            "apps": {
                "0": {
                    "path": "a2a_b.py",
                    "args": "-msgsize 1073741824 -iter 10",
                    "collect": true,
                    "partition": "0",
                    "start": "0",
                    "end": ""
                }
            }
        },
        "09_dep_sequential_chain": {
            "description": "Test Dipendenze: App 1 parte SOLO quando App 0 ha finito (Chain).",
            "apps": {
                "0": {
                    "path": "a2a_b.py",
                    "args": "-msgsize 1048576 -iter 50",
                    "collect": true,
                    "partition": "0",
                    "start": "0",
                    "end": ""
                },
                "1": {
                    "path": "ring_nb.py",
                    "args": "-msgsize 1048576 -iter 50",
                    "collect": true,
                    "partition": "0",
                    "start": "s0",
                    "end": ""
                }
            }
        },
        "10_dep_parallel_delayed": {
            "description": "Test Temporale: App 0 parte a T=0, App 1 parte a T=5s (Parallel execution).",
            "apps": {
                "0": {
                    "path": "a2a_b.py",
                    "args": "-msgsize 1048576 -iter 200",
                    "collect": true,
                    "partition": "0",
                    "start": "0",
                    "end": ""
                },
                "1": {
                    "path": "inc_b.py",
                    "args": "-msgsize 1048576 -iter 200",
                    "collect": true,
                    "partition": "0",
                    "start": "5",
                    "end": ""
                }
            }
        },
        "11_dep_killer_aggressor": {
            "description": "Test Aggressore 'f': L'aggressore (App 1) viene ucciso quando la Vittima (App 0) finisce.",
            "apps": {
                "0": {
                    "path": "a2a_b.py",
                    "args": "-msgsize 1048576 -iter 100",
                    "collect": true,
                    "partition": "0",
                    "start": "0",
                    "end": ""
                },
                "1": {
                    "path": "inc_b.py",
                    "args": "-msgsize 65536 -iter 1000000",
                    "collect": false,
                    "partition": "0",
                    "start": "0",
                    "end": "f"
                }
            }
        },
        "12_dep_killer_chain": {
            "description": "Chain Killer: App 1 parte dopo App 0, App 2 vive finchè App 1 vive.",
            "apps": {
                "0": {
                    "path": "barrier.py",
                    "args": "-iter 1000",
                    "collect": false,
                    "partition": "0",
                    "start": "0",
                    "end": ""
                },
                "1": {
                    "path": "a2a_b.py",
                    "args": "-msgsize 1048576 -iter 50",
                    "collect": true,
                    "partition": "0",
                    "start": "s0",
                    "end": ""
                },
                "2": {
                    "path": "inc_b.py",
                    "args": "-msgsize 65536 -iter 1000000",
                    "collect": false,
                    "partition": "0",
                    "start": "s0",
                    "end": "f"
                }
            }
        },
