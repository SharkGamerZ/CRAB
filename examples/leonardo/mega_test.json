{
    "global_options": {
        "numnodes": "256",
        "ppn": "1",
        "allocationmode": "p",
        "partitionsplit": "e",
        "allocationsplit": "e",
        "minruns": "3",
        "maxruns": "5",
        "timeout": "10800.0",
        "convergeall": true,
        "outformat": "csv",
        "extrainfo": "256nodes_comprehensive_test",
        "sbatch_directives": {
            "account": "IscrB_SWING",
            "partition": "boost_usr_prod",
            "exclusive": true,
            "time": "02:00:00",
            "job-name": "crab_256n_scale"
        }
    },
    "experiments": {
        "01_scale_a2a_1KB": {
            "description": "Scalability Test 1/8: All-to-All su 256 nodi, 1KB msg size.",
            "apps": {
                "0": {
                    "path": "a2a_b.py",
                    "args": "-msgsize 1024 -iter 1000",
                    "collect": true,
                    "partition": "0",
                    "start": "0",
                    "end": ""
                }
            }
        },
        "02_scale_a2a_4KB": {
            "description": "Scalability Test 2/8: 4KB msg size.",
            "apps": {
                "0": {
                    "path": "a2a_b.py",
                    "args": "-msgsize 4096 -iter 1000",
                    "collect": true,
                    "partition": "0",
                    "start": "0",
                    "end": ""
                }
            }
        },
        "03_scale_a2a_64KB": {
            "description": "Scalability Test 3/8: 64KB msg size.",
            "apps": {
                "0": {
                    "path": "a2a_b.py",
                    "args": "-msgsize 65536 -iter 500",
                    "collect": true,
                    "partition": "0",
                    "start": "0",
                    "end": ""
                }
            }
        },
        "04_scale_a2a_1MB": {
            "description": "Scalability Test 4/8: 1MB msg size.",
            "apps": {
                "0": {
                    "path": "a2a_b.py",
                    "args": "-msgsize 1048576 -iter 200",
                    "collect": true,
                    "partition": "0",
                    "start": "0",
                    "end": ""
                }
            }
        },
        "05_scale_a2a_4MB": {
            "description": "Scalability Test 5/8: 4MB msg size.",
